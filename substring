my_str = "abcdef"

str_list = []
str_list[:0] = my_str

strcount = 0
range1 = []
for l in str_list:
  range1.append(strcount)
  strcount += 1

range2 = range1[1:strcount]
range2.append(strcount)

for i in range1:
  for j in range2:
      if i != j & j>i:
        print(my_str[i:j])
      else:
        pass





