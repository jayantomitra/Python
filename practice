#print last element of of basestring
def convertstring(string):
  list1 = []
  list1[:0]= string
  return list1

#driver code
my_str = "Hello world"

list2 = convertstring(my_str)
print(list2)

l = len(list2)
  
for i in range(l):
  if i == l-5:
    print(list2[i])


# python program to print substrings of a substrings

my_string = 'abcd'

length = len(my_string)

for i in range(length):
  for j in range(i+1, length+1):
    print(my_string[i:j])
    
    
